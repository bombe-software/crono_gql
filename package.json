    "lodash": "^4.17.15",
    "mongoose": "^5.6.8"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "rimraf": "^2.6.3",
    "nodemon": "^1.19.1"
  }
}
